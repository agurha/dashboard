{
  "name": "marcins-dashboard",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "NODE_PATH=. babel-node server.js",
    "dev": "npm run dev:front-end & npm run dev:back-end",
    "dev:front-end": "NODE_PATH=. webpack-dev-server --progress --profile --colors --hot",
    "dev:back-end": "NODE_PATH=. nodemon server.js --exec babel-node",
