    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "classnames": "2.2.5",
    "css-loader": "0.23.1",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.12.0",
    "moment": "2.13.0",
    "node-sass": "3.7.0",
    "normalize.css": "4.2.0",
